{
  "name": "suuq-backend",
  "version": "1.0.0",
  "description": "Suuq backend API",
  "author": "Ugas Fuad",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "build": "nest build",
    "clean": "rm -rf dist",
    "start": "node dist/main.js",
    "start:prod": "NODE_ENV=production node dist/main.js",
    "start:dev": "nest start --watch",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "db:rebuild-closure": "ts-node -r tsconfig-paths/register --transpile-only scripts/rebuild-closure.ts",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "seed": "ts-node src/seeds/main.ts",
    "seed:countries": "ts-node src/seeds/main.ts countries",
    "seed:admin": "node --env-file=.env -r ts-node/register src/seeds/create-admin.ts",
    "typeorm": "node --env-file=.env -r ts-node/register -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d src/data-source.ts",
    "seed:products": "ts-node src/seeds/create-products.ts",
  "seed:categories": "ts-node src/database/seeds/seed-categories.ts",
  "backfill:sales": "ts-node -r tsconfig-paths/register --transpile-only scripts/backfill-product-sales.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.862.0",
  "@aws-sdk/s3-request-presigner": "^3.864.0",
    "@nestjs/common": "^11.1.3",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.5",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/typeorm": "^11.0.0",
    "@types/axios": "^0.14.4",
    "axios": "^1.10.0",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.0",
    "firebase-admin": "^13.4.0",
    "google-auth-library": "^9.15.1",
    "multer": "^2.0.2",
    "multer-s3": "^3.0.1",
    "nodemailer": "^7.0.5",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "sharp": "^0.34.3",
    "swagger-ui-express": "^5.0.1",
    "transliteration": "^2.3.5",
    "typeorm": "^0.3.25"
  },
  "devDependencies": {
    "@aws-sdk/s3-request-presigner": "^3.864.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@types/aws-sdk": "^2.7.4",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.2",
    "@types/jest": "^30.0.0",
    "@types/multer": "^2.0.0",
    "@types/multer-s3": "^3.0.3",
    "@types/node": "^24.1.0",
    "@types/nodemailer": "^6.4.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/pg": "^8.15.4",
    "@types/sharp": "^0.32.0",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
